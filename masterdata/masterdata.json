{
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema",
  "entities": {
    "affiliates_affiliates": {
      "schemas": {
        "2.3.0": {
          "$id": "affiliates_affiliates@2.3.0",
          "title": "Affiliates",
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "email": { "type": "string" },
            "slug": { "type": "string", "maxLength": 255 }
          },
          "required": ["name", "email", "slug"],
          "v-indexed": ["name", "email", "slug"],
          "v-cache": false,
          "v-triggers": []
        }
      }
    },
    "commissionBySKU": {
      "schemas": {
        "2.3.0": {
          "$id": "commissionBySKU@2.3.0",
          "title": "Commission By SKU",
          "type": "object",
          "properties": {
            "commission": { "type": "number" },
            "refId": { "type": "string" }
          },
          "required": ["commission"],
          "v-indexed": ["refId", "commission"],
          "v-cache": false,
          "v-triggers": []
        }
      }
    },
    "affiliatesOrders": {
      "schemas": {
        "2.3.0": {
          "$id": "affiliatesOrders@2.3.0",
          "title": "AffiliatesOrders",
          "type": "object",
          "properties": {
            "affiliateId": { "type": "string" },
            "status": { "type": "string" },
            "userEmail": { "type": "string" },
            "orderTotal": { "type": "number" },
            "orderTotalCommission": { "type": "number" },
            "orderDate": { "type": "string", "format": "date-time" },
            "orderItems": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "skuId": { "type": "string" },
                  "skuName": { "type": "string" },
                  "skuImageUrl": { "type": "string" },
                  "price": { "type": "number" },
                  "quantity": { "type": "number" },
                  "commission": { "type": "number" }
                },
                "required": ["skuId", "skuName", "price", "quantity", "commission"]
              }
            }
          },
          "required": ["affiliateId", "userEmail", "orderItems"],
          "v-indexed": [
            "affiliateId",
            "userEmail",
            "status",
            "orderDate",
            "orderTotal",
            "orderTotalCommission"
          ],
          "v-cache": false,
          "v-triggers": []
        }
      }
    }
  }
}